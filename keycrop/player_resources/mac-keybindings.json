[
    {
        "key": "cmd+shift+p",
        "command": "runCommands",
        "args": {
            "commands": [
                "workbench.action.showCommands",
                "keycrop.growBean"
            ]
        }
    },
    {
        "key": "cmd+shift+space",
        "command": "runCommands",
        "args": {
            "commands": [
                {
                    "command": "editor.action.triggerParameterHints",
                    "when": "editorHasSignatureHelpProvider && editorTextFocus"
                },
                "keycrop.growChili"
            ]
        }
    },
    {
        "key": "cmd+\\",
        "command": "runCommands",
        "args": {
            "commands": [
                "workbench.action.splitEditor",
                "keycrop.growBroccoli"
            ]
        }
    },
    {
        "key": "cmd+/",
        "command": "runCommands",
        "args": {
            "commands": [
                {
                    "command": "editor.action.commentLine",
                    "when": "editorTextFocus && !editorReadonly"
                },
                "keycrop.growLettuce"
            ]
        }
    }
]